{% extends 'base.html.twig' %}
{% block form %}
<div class="form-block__content">

{% if content.form %}
        {% if app.request.get('send') != 'true' %}
            {# FORM THEME #}
            {% form_theme content.form 'form/theme.html.twig' %}
            {{ form(content.form) }}
        {% else %}
            {{ view.form.entity.successText|raw }}
        {% endif %}
    {% endif %}
</div>
{% endblock %}

{% block content %}
    <div class="google-maps">
        <iframe
                frameborder="0" style="border:0"
                src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBwCWWqy9bguskdVxdWwZz9NP5MSIhEBVc&q=49.435073, 20.939629&zoom=18" allowfullscreen>
        </iframe>
    </div>
{% endblock %}
